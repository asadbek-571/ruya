    <version>0.0.1</version>
    <name>ruya</name>
    <description>Ruya Mobile</description>
    <packaging>jar</packaging>

    <properties>
        <java.version>11</java.version>
        <mapstruct.version>1.5.3.Final</mapstruct.version>
